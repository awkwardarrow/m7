# Export single table from backup
# (xtrabackup with --prepare --export)
--enable_quiet_exec
CREATE TABLE t(i INT) ENGINE INNODB;
INSERT INTO t VALUES(1);
echo # xtrabackup backup;
let $targetdir=$MYSQLTEST_VARDIR/tmp/backup;
exec $XTRABACKUP --backup --target-dir=$targetdir ;

INSERT INTO t VALUES(2);

echo # xtrabackup prepare;
exec $XTRABACKUP --prepare --export --target-dir=$targetdir;


ALTER TABLE t DISCARD TABLESPACE;
let $MYSQLD_DATADIR= `select @@datadir`;
copy_file $targetdir/test/t.ibd $MYSQLD_DATADIR/test/t.ibd;
copy_file $targetdir/test/t.cfg $MYSQLD_DATADIR/test/t.cfg;
ALTER TABLE t IMPORT TABLESPACE;

SELECT * FROM t;
DROP TABLE t;
force-rmdir $targetdir;
--disable_quiet_exec
